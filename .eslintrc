{
  "extends": [
    "eslint:recommended",
    "v2land"
  ],
  "parser": "typescript-eslint-parser",
  "plugins": [
    "typescript"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
  },
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  }
}